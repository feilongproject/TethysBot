{
    "admin": {
        "identityList": {
            "reg": "^添加权限",
            "fnc": "addAdmin",
            "type": ["GUILD"],
            "describe": "对指定用户添加权限"
        },
        "testShell": {
            "reg": "^运行命令(.*)",
            "fnc": "testShell",
            "type": ["GUILD", "DIRECT"],
            "describe": "测试shell功能"
        }
    },
    "identity": {
        "identityList": {
            "reg": "^(身份组列出|列出身份组)$",
            "fnc": "identityList",
            "type": ["GUILD"],
            "describe": "列出身份组"
        },
        "identitySet": {
            "reg": "^(设置身份组|身份组设置)",
            "fnc": "identitySet",
            "type": ["GUILD"],
            "describe": "身份组设置"
        },
        "identityInfo": {
            "reg": "^(设置信息|信息设置)",
            "fnc": "identityInfo",
            "type": ["GUILD"],
            "describe": "表情分配设置"
        },
        "warnWithReason": {
            "reg": "^警告(.+)\\s?原因(.+)",
            "fnc": "warnWithReason",
            "type": ["GUILD"],
            "describe": "警告@xxx原因yyy"
        }
    },
    "another": {
        "getRandomNums": {
            "reg": "^获取随机数\\s?(\\d+)\\s(\\d+)\\s(\\d+)$",
            "fnc": "getRandomNums",
            "type": ["GUILD", "DIRECT"],
            "describe": "获取随机数xxx yyy zzz"
        },
        "getUID": {
            "reg": "^(获取)?(.*)?UID$",
            "fnc": "getUID",
            "type": ["GUILD", "DIRECT"],
            "describe": "获取当前用户或指定用户UID"
        }
    },
    "scoreboard": {
        "scoreboardQuery": {
            "reg": "^我的信息$",
            "fnc": "scoreboardQuery",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "查询当前积分"
        },
        "scoreboardRankList": {
            "reg": "^(.+)排名$",
            "fnc": "scoreboardRankList",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "列出指定榜单的排名"
        },
        "scoreboardRankListMy": {
            "reg": "^我的榜单$",
            "fnc": "scoreboardRankListMy",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "列出自己所有的榜单积分"
        },
        "scoreboardList": {
            "reg": "^列出榜单$",
            "fnc": "scoreboardList",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "列出所有榜单名称"
        },
        "scoreboardCreate": {
            "reg": "^(添加|删除)(.+)榜单$",
            "fnc": "scoreboardCreate",
            "type": ["GUILD", "DIRECT"],
            "describe": "（管理员）添加或删除指定榜单"
        },
        "scoreboardChange": {
            "reg": "^(添加|扣除)\\s*(<@!?\\d+>\\s*)+(\\d+)(.+)$",
            "fnc": "scoreboardChange",
            "type": ["GUILD"],
            "describe": "（管理员）操作某人指定榜单的积分"
        },
        "scoreboardSetAnswer": {
            "reg": "^答案(.+)积分(\\d+)人数(\\d+)$",
            "fnc": "scoreboardSetAnswer",
            "type": ["DIRECT"],
            "describe": "（管理员）根据答案分配积分给指定榜单"
        },
        "scoreboardChangeWithIdentity": {
            "reg": "^(添加|扣除)(.+)身份组(\\d+)(积分)?$",
            "fnc": "scoreboardChangeWithIdentity",
            "type": ["GUILD"],
            "describe": "(暂无法使用)操作某身份组积分"
        },
        "scoreboardAnswer": {
            "reg": ".*",
            "fnc": "scoreboardAnswer",
            "type": ["DIRECT"],
            "permission": "anyone",
            "describe": "根据答案获取积分"
        }
    },
    "unionManager": {
        "createUnion": {
            "reg": "^创建公会(.+)",
            "fnc": "createUnion",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "创建一个公会（必须需要名称）"
        },
        "getUnionInformation": {
            "reg": "^(.*)公会信息$",
            "fnc": "getUnionInformation",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "获取公会信息（可指定公会名称）"
        },
        "getUnionRank": {
            "reg": "^公会排行$",
            "fnc": "getUnionRank",
            "type": ["GUILD", "DIRECT"],
            "permission": "anyone",
            "describe": "获取积分在前十的公会的排行榜"
        },
        "changeUnionScore": {
            "reg": "^(增加|扣减)(.+[^\\d])(\\d+)积分$",
            "fnc": "changeUnionScore",
            "type": ["GUILD", "DIRECT"],
            "describe": "（管理员）对指定公会的积分进行操作"
        },
        "inviteJoinUnion": {
            "reg": "^邀请.+(加入公会|入会)$",
            "fnc": "inviteJoinUnion",
            "type": ["GUILD"],
            "permission": "anyone",
            "describe": "（会长）邀请某人加入发送者所在公会"
        },
        "inviteJoinUnionUser": {
            "reg": "^(确认|拒绝)加入$",
            "fnc": "inviteJoinUnionUser",
            "type": ["GUILD"],
            "permission": "anyone",
            "describe": "被邀请者可以确认/拒绝公会邀请"
        }
    }
}
